@model TimeSheet.Models.UserBase
@{
    ViewBag.Title = "Session not available";
}

<hgroup class="title">
    <h2>@ViewBag.Title.</h2>
    <h2>@ViewBag.Message</h2>
</hgroup>

<section class="contact">
    <header>
        <h3>Your identity @Model.User is not in the RDSS Timesheet database or current session.</h3>
        <h4>Please try closing and opening your browser to reestablish a session.  (Make sure you are logged in.)</h4>
    </header>
</section>
